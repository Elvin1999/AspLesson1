@model WebApplication1.Models.PersonListViewModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index3</title>

</head>
<body>
    <h1>Persons</h1>
    @foreach (var item in Model.Persons)
    {
        <p>@item.Id</p>
        <p>@item.Name</p>
        <p>@item.Surname</p>
        <p>@item.CityId</p>
        <hr>
    }

    <h1>Cities</h1>

    @foreach(var c in Model.Cities)
            {
               <h2>@c</h2>
            }  
    
</body>
</html>
